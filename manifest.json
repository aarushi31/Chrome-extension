{
    "manifest_version":2,
    "name":"Social Media Launcher",
    "description":"Quick launch my social media",
    "version":"1.0.0",
    "content_scripts":[
        {
            "matches":[
                "<all_urls>"
            ],
            "js":["popup.js"]
        }
    ],
    "icons":{"128":"icon2.png"},
    "browser_action":{
        "default_icon":"icon.png",
        "default_popup":"popup.html"
    },
    "permissions":["activeTab"]
}